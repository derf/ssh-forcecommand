disk_root       = /usr/lib/nagios/plugins/check_disk -w 12% -c 5% -p /
disk_home       = /usr/lib/nagios/plugins/check_disk -w 5% -c 2% -p /home
disk_boot       = /usr/lib/nagios/plugins/check_disk -w 12% -c 5% -p /boot
libs            = /usr/bin/sudo /usr/lib/nagios/plugins/check_libs
load            = /usr/lib/nagios/plugins/check_load -w 15,10,5 -c 30,25,20
processes       = /usr/lib/nagios/plugins/check_procs -w 150 -c 200
raid            = /usr/lib/nagios/plugins/check_raid
users           = /usr/lib/nagios/plugins/check_users -w 14 -c 20
mailq_postfix   = /usr/lib/nagios/plugins/check_mailq -w 10 -c 20 -M postfix
packages        = /usr/lib/nagios/plugins/check_packages
kernel          = /usr/lib/nagios/plugins/check_kernel
certificate     = /usr/lib/nagios/plugins/check_cert_expire /etc/ssl/chaosdorf.crt
git_etc         = /usr/bin/sudo /usr/lib/nagios/plugins/check_git_status /etc
git_usr_local   = /usr/bin/sudo /usr/lib/nagios/plugins/check_git_status /usr/local
git_srv_roundup = /usr/bin/sudo /usr/lib/nagios/plugins/check_git_status /srv/roundup
smart_sda       = /usr/bin/sudo /usr/lib/nagios/plugins/check_ide_smart -d /dev/sda -n
smart_sdb       = /usr/bin/sudo /usr/lib/nagios/plugins/check_ide_smart -d /dev/sdb -n
smart_hda       = /usr/bin/sudo /usr/lib/nagios/plugins/check_ide_smart -d /dev/hda -n
smart_hdc       = /usr/bin/sudo /usr/lib/nagios/plugins/check_ide_smart -d /dev/hdc -n
