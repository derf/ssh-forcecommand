packages  = dpkg --get-selections
etc       = tar -C / -cf - etc
home      = tar -C / -cf - home
srv       = tar -C / -cf - srv
root      = tar -C / -cf - root
var_log   = tar -C / -cf - var/log
usr_local = tar -C / -cf - usr/local
var_local = tar -C / -cf - var/local

ldap      = slapcat
sqldump   = sh sqldump.sh
